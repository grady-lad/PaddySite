body
  font-family "Arial" !important

#wrapper
  margin-top 10px

// when viewing through iphone stuff
// get vertical center (well, kinda)
@media (max-width: 320px)
  #wrapper
    margin-top 40%

@media (max-width: 725px)
  .nextUrl
    opacity 0
  .prevUrl
    opacity 0

/* submission-form.jade */
#feedback
  margin-top 50px
  z-index 1

#email
  width 10px
  margin-bottom 10px

#suggestion
  width 250px

#submit_button
  margin-top 10px

@media (min-width: 790px)
  #contactsuggestion, #contactemail
    width 200%
    
@media (min-width: 790px)
  #loginsuggestion, #loginemail
    width 100%

#errorMessage
  z-index -10
  color #E81518
  margin-top 10px
  font-weight 200
  margin-left 3%

#successMessage
  z-index -10
  color #15E8A2
  margin-top 10px
  font-weight 200
  width 200px
  margin-left 3%
 
.app
  min-height 300px
.about-text
   font-family apple_regular

.nav-text
  font-family Super-Effective
  color #FF3C77
  font-size 28px
  
.upload-nav-text
  font-size 36px
  font-family Courier New
  color black
  
.going-right
  margin-left 80%

.lil-extra
  margin-left 20px

highlight
  color #FF3C77	
  
highlight a
  color #FF3C77
  
.logo_and_navbar
  margin-top 25px

.nav-logo
  margin-top -15px !important

.footer
   padding-top 100px

.footer p
   float right     

  #intialContent
    display none
    
.img-centre
   margin 0 auto
   animation fadein 1s
   -moz-animation fadein 1s
   -webkit-animation fadein 1s
   -o-animation fadein 1s

@keyframes fadein
    from 
        opacity:0;
    to 
        opacity:1;
    
@-moz-keyframes fadein
  from
    opacity 0 
  to
    opacity 1
    
@-webkit-keyframes fadein
  from
    opacity 0
  to 
    opacity 1

@-o-keyframes fadein
  from
    opacity 0  
  to
    opacity 1
  
.singleIll
  position relative
 
 .centre-links
   margin-top 20px
   text-align center

.cloudinary_fileupload
  color transparent

.illRow
  margin-top 20px
    
.loading
  position fixed
  opacity 0.1px
  left 0px
  top 0px
  width 100%
  height 100%
  z-index 9999
  background url('../images/loading.gif') 50% 50% no-repeat 
  
.centerIll
  width 300px; !important
  height 400px;
  max-height 400px
  overflow hidden
    
.uploader-panel
 border 4px solid #eee
 
.uploadbutton
  margin-top 10px

.prevUrl
  top 40%
  display inline
  left 60px
  position fixed
  

.nextUrl
  top 40%
  display inline
  right 60px
  position fixed