.centerIll
  height 300px
  width 300px
  overflow hidden
    
.uploader-panel
 border 4px solid #eee
 
.uploadbutton
  margin-top 10px

//Arrow mixin
arrowNav()
  top 40%
  display inline
  position fixed
  
.prevUrl
  left 50px
  arrowNav()
  
.nextUrl
  right 60px
  arrowNav()

maxImage()
  max-width 100%
  max-height 100%

.multipleIll
  position relative
  opacity 0
  max-width 100%

.uploadImage
  max-heigth 100%
  max-width 100%

.singleIll
  position relative
  maxImage()
  margin 0 auto
  opacity 0

.fadeIn
  -webkit-transition opacity 0.5s ease-in
  -moz-transition opacity 0.5s ease-in
  -o-transition opacity 0.5s ease-in
  opacity 1

.modalContainer
  position fixed
  bottom: 0
  left 0
  right 0
  top 0
  background rgba(0,0,0,.8)
  display none
  z-index 9000

.modalCustom
  width 800px
  height 600px
  background-color #fff
  position fixed
  top 25%
  left 40%
  margin -200px 0 0 -200px

.close
 position inherit
 right 5
 top 5
 
 .remove-image
   width 33%
 
.remove-image figure
  width 100%
  height 100%
  position: relative;
 
.remove-image figcaption
  background rgba(0,0,0,0.8)
  color white
  display table
  left 0
  right 0
  top 0
  opacity 0
  position absolute
  z-index 100
  width 100%
  height 225px

.remove-image p
  cursor pointer
  float left

.editImage:not(.no-hover):hover 
  opacity: 1
  
.crop
  position absolute
  bottom 0
  left 20%
  font-size 3.5em

.crop:hover
  color #FF3C77

.remove:hover
  color #FF3C77
    

.remove
  position absolute
  top 0
  left 5%
  font-size 3.5em

.cropper
  position relative
  max-height 100%
  max-width 100%

.closure
  position absolute
  top 0
  right 0
  font-size 9.5em
  color #FF3C77

.closure:hover
  color #FF3C77

.crop-controls
  margin 0 auto
  position relative
  padding-top 80px
  width 100%

.crop-control
  float left
  width 33.3%
  margin 0 auto
  padding-right 10px
  padding-left 5px
  
.saveCrop
  position absolute
  bottom 25px
  right 10px

.imageWrap 
  width: 300px; 
  height: 300px; 
  background: url('https://s3-eu-west-1.amazonaws.com/paddystaging/loading.gif') center center no-repeat;
  
.btner
  outline none
  width 150px
  text-align center
  color white
  border none
  border-radius 10px
  margin-top 10px
  background-color #FF3C77
  height 30px
  font-size 16px
  box-shadow: 0 5px 5px #FF3C94;
  &:active
    box-shadow 0 2px 0 #FF3C77
    //We are actually moving the element down
    transform translateY(3px)
  
.dropDownGravityList select
  background-color #FF3C77
  color white
  border none
  border-radius 10px
  font-size 16px
  overflow hidden
  width 150px
  height 30px
  outline none


.dropDownCropList input
  background-color #FF3C77
  color white
  border none
  border-radius 10px
  font-size 16px
  overflow hidden
  width 150px
  height 30px
  outline none
  
  &::-webkit-input-placeholder
   color white
   
  &:-moz-placeholder  /* Firefox 18- */
    color white

  &::-moz-placeholder   /* Firefox 19+ */
   color white

  &:-ms-input-placeholder  
   color white

.cropPrev
  margin-bottom 300px !important

.mog
  text-align right

.modalImageWrapper
  position relative
  margin 0 auto
  padding-top 70px
  height 360px
  width 360px
  background url('https://s3-eu-west-1.amazonaws.com/paddystaging/loading.gif') 40% 50% no-repeat

