#intialContent
  display none

.singleHolder
  height 600px
  background url('https://s3-eu-west-1.amazonaws.com/paddystaging/loading.gif') 50% 50% no-repeat
 
.cloudinary_fileupload
  color transparent

.illRow
  margin-top 20px
    
.loading
  position fixed
  opacity 0.1px
  left 0px
  top 0px
