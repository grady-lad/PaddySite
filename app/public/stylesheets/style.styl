#wrapper
  margin-top 50px

// when viewing through iphone stuff
// get vertical center (well, kinda)
@media (max-width: 320px)
  #wrapper
    margin-top 40%

@media (max-width: 725px)
  .nextUrl
    opacity 0
  .prevUrl
    opacity 0

@media (max-width: 500px)
 .singleHolder
   height 300px !important

 #social
   display inline-block

/* submission-form.jade */
#feedback
  margin-top 50px
  z-index 1

#email
  width 10px
  margin-bottom 10px

#suggestion
  width 250px

#submit_button
  margin-top 10px
  position relative
  margin-left 140%
  border 1px solid black
  width 60%
  font-family apple_regular

#contactsuggestion, #contactemail
  border 1px solid black
  font-family apple_regular

@media (min-width: 993px)
  #contactsuggestion, #contactemail
    width 200%

@media (max-width: 990px)
  #contactsuggestion, #contactemail
    width 100%
  #submit_button
    margin 0 0 0 0
    width 30%
    
@media (min-width: 790px)
  #loginsuggestion, #loginemail
    width 100%

messageLayout()
  z-index -10
  margin-top 10px
  font-weight 200
  margin-left 3%

#errorMessage
  messageLayout()
  color #E81518

#successMessage
  messageLayout()
  color #15E8A2
 
